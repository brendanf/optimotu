@article{vu2016,
  title = {{{DNA}} Barcoding Analysis of More than 9\hphantom{,}000 Yeast Isolates Contributes to Quantitative Thresholds for Yeast Species and Genera Delimitation},
  author = {Vu, D. and Groenewald, M. and Sz{\"o}ke, S. and Cardinali, G. and Eberhardt, U. and Stielow, B. and {\noopsort{vries}}{de Vries}, M. and Verkleij, G. J. M. and Crous, P. W. and Boekhout, T. and Robert, V.},
  year = {2016},
  month = sep,
  journal = {Studies in Mycology},
  volume = {85},
  pages = {91--105},
  issn = {0166-0616},
  doi = {10.1016/j.simyco.2016.11.007},
  urldate = {2019-09-20},
  abstract = {DNA barcoding is a global initiative for species identification through sequencing of short DNA sequence markers. Sequences of two loci, ITS and LSU, were generated as barcode data for all (ca. 9k) yeast strains included in the CBS collection, originally assigned to ca. 2\hphantom{,}000 species. Taxonomic sequence validation turned out to be the most severe bottleneck due to the large volume of generated trace files and lack of reference sequences. We have analysed and validated CBS strains and barcode sequences automatically. Our analysis shows that there were 6 and 9.5~\% of CBS yeast species that could not be distinguished by ITS and LSU, respectively. Among them, {$\sim$}3~\% were indistinguishable by both loci. Except for those species, both loci were successfully resolving yeast species as the grouping of yeast DNA barcodes with the predicted taxonomic thresholds was more than 90~\% similar to the grouping with respect to the expected taxon names. The taxonomic thresholds predicted to discriminate yeast species were 98.41~\% for ITS and 99.51~\% for LSU. To discriminate current yeast genera, thresholds were 96.31~\% for ITS and 97.11~\% for LSU. Using ITS and LSU barcodes, we were also able to show that the recent reclassifications of basidiomycetous yeasts in 2015 have made a significant improvement for the generic taxonomy of those organisms. The barcodes of 4\hphantom{,}730 (51~\%) CBS yeast strains of 1\hphantom{,}351 (80~\%) accepted yeast species that were manually validated have been released to GenBank and the CBS-KNAW website as reference sequences for yeast identification.},
  keywords = {Automated curation,D1/D2,Fungi,ITS,LSU,Taxonomy,Yeast},
  file = {/home/brfurnea/gdrive/Papers/Lab Methods/Molecular Biology/Barcoding/Vu_etal_2016_DNA_barcoding_analysis_of_more_than_9_000_yeast_isolates_contributes_to(Studies_in_Mycology).pdf;/home/brfurnea/Zotero/storage/S9ABE8ZQ/S0166061616300215.html}
}
