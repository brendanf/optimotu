@article{codjia2020,
  title = {Morphology, Multilocus Phylogeny, and Toxin Analysis Reveal {{{\emph{Amanita}}}}{\emph{ Albolimbata}}, the First Lethal {{{\emph{Amanita}}}} Species {{From Benin}}, {{West Africa}}},
