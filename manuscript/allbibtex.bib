@article{anslan2017,
  title = {{{PipeCraft}}: {{Flexible}} Open-Source Toolkit for Bioinformatics Analysis of Custom High-Throughput Amplicon Sequencing Data},
  shorttitle = {{{PipeCraft}}},
  author = {Anslan, Sten and Bahram, Mohammad and Hiiesalu, Indrek and Tedersoo, Leho},
