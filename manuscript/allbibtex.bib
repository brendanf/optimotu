@article{storkHowManySpecies2018,
  title = {How {{Many Species}} of {{Insects}} and {{Other Terrestrial Arthropods Are There}} on {{Earth}}?},
  author = {Stork, Nigel E.},
  year = {2018},
  month = jan,
  journal = {Annual Review of Entomology},
