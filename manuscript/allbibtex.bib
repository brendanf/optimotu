@article{tian2017,
  title = {Patterns and Drivers of Fungal Diversity along an Altitudinal Gradient on {{Mount Gongga}}, {{China}}},
  author = {Tian, Jianqing and Wu, Bing and Chen, Huai and Jiang, Na and Kang, Xiaoming and Liu, Xingzhong},
