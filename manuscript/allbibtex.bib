@article{voriskova2017,
  title = {Real-Time {{PCR}} Quantification of Arbuscular Mycorrhizal Fungi: Does the Use of Nuclear or Mitochondrial Markers Make a Difference?},
  shorttitle = {Real-Time {{PCR}} Quantification of Arbuscular Mycorrhizal Fungi},
  author = {Vo{\v r}{\'i}{\v s}kov{\'a}, Alena and Jansa, Jan and P{\"u}schel, David and Kr{\"u}ger, Manuela and Cajthaml, Tom{\'a}{\v s} and Vos{\'a}tka, Miroslav and Janou{\v s}kov{\'a}, Martina},
