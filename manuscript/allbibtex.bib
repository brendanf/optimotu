@article{kesel2016,
  ids = {kesel2016a},
  title = {New and {{Interesting}} {{{\emph{Cantharellus}}}} from {{Tropical Africa}}},
  author = {De Kesel, Andr{\'e} and Amalfi, Mario and Ngoy, Bill Kasongo Wa and Yorou, Nourou S. and Rasp{\'e}, Olivier and Degreef, J{\'e}r{\^o}me and Buyck, Bart},
