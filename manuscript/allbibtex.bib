@article{degreef2016,
  title = {Wild Edible Mushrooms, a Valuable Resource for Food Security and Rural Development in {{Burundi}} and {{Rwanda}}},
  author = {Degreef, J{\'e}r{\^o}me and Demuynck, Laurent and Mukandera, Assumpta and Nyirandayambaje, Gudula and Nzigidahera, Beno{\^i}t and De~Kesel, Andr{\'e}},
  year = {2016},
  month = jan,
