  title = {Leveraging Single-Cell Genomics to Expand the Fungal Tree of Life},
  author = {Ahrendt, Steven R. and Quandt, C. Alisha and Ciobanu, Doina and Clum, Alicia and Salamov, Asaf and Andreopoulos, Bill and Cheng, Jan-Fang and Woyke, Tanja and Pelin, Adrian and Henrissat, Bernard and Reynolds, Nicole K. and Benny, Gerald L. and Smith, Matthew E. and James, Timothy Y. and Grigoriev, Igor V.},
