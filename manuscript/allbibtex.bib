  author = {Chai, Hui and Liang, Zhi-Qun and Xue, Rou and Jiang, Shuai and Luo, Shi-Hong and Wang, Yong and Wu, Lu-Ling and Tang, Li-Ping and Chen, Yun and Hong, Deng and Zeng, Nian-Kai},
