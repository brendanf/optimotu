@article{chen2015,
  title = {Next Generation Multilocus Sequence Typing ({{NGMLST}}) and the Analytical Software Program {{MLSTEZ}} Enable Efficient, Cost-Effective, High-Throughput, Multilocus Sequencing Typing},
  author = {Chen, Yuan and Frazzitta, Aubrey E. and Litvintseva, Anastasia P. and Fang, Charles and Mitchell, Thomas G. and Springer, Deborah J. and Ding, Yun and Yuan, George and Perfect, John R.},
  year = {2015},
