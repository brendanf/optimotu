@article{moreau2015,
  title = {{\emph{Cibaomyces}} and {{{\emph{Cyptotrama}}}}, Two New Genera for {{Europe}}, and an Emendation of {{{\emph{Rhizomarasmius}}}} ({{Basidiomycota}}, {{Physalacriaceae}})},
  author = {Moreau, Pierre-Arthur and Vila, Jordi and Aime, M. Catherine and Anton{\'i}n, Vladim{\'i}r and Horak, Egon and {P{\'e}rez-Butr{\'undefined}n}, Jos{\'e} Luis and Richard, Franck and Urban, Alexander and Welti, St{\'e}phane and Vizzini, Alfredo},
