@article{dutilh2011,
  title = {{{FACIL}}: {{Fast}} and {{Accurate Genetic Code Inference}} and {{Logo}}},
  shorttitle = {{{FACIL}}},
  author = {Dutilh, Bas E. and Jurgelenaite, Rasa and Szklarczyk, Radek and {\noopsort{hijum}}{van Hijum}, Sacha A.F.T. and Harhangi, Harry R. and Schmid, Markus and {\noopsort{wild}}{de Wild}, Bart and Fran{\c c}oijs, Kees-Jan and Stunnenberg, Hendrik G. and Strous, Marc and Jetten, Mike S.M. and {Op den Camp}, Huub J.M. and Huynen, Martijn A.},
