@article{gohar2022,
  title = {Global Diversity and Distribution of Mushroom-Inhabiting Bacteria},
  author = {Gohar, Daniyal and P{\~o}ldmaa, Kadri and Tedersoo, Leho and Aslani, Farzad and Furneaux, Brendan and Henkel, Terry W. and Saar, Irja and Smith, Matthew E. and Bahram, Mohammad},
