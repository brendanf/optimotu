@article{davison2021,
  title = {Temperature and {{pH}} Define the Realised Niche Space of Arbuscular Mycorrhizal Fungi},
  author = {Davison, John and Moora, Mari and Semchenko, Marina and Adenan, Sakeenah Binte and Ahmed, Talaat and Akhmetzhanova, Asem A. and Alatalo, Juha M. and {Al-Quraishy}, Saleh and Andriyanova, Elena and Anslan, Sten and Bahram, Mohammad and Batbaatar, Amgaa and Brown, Charlotte and Bueno, C. Guillermo and Cahill, James and Cantero, Juan Jos{\'e} and Casper, Brenda B. and Cherosov, Mikhail and Chideh, Saida and Coelho, Ana P. and Coghill, Matthew and Decocq, Guillaume and Dudov, Sergey and Fabiano, Ezequiel Chimbioputo and Fedosov, Vladimir E. and Fraser, Lauchlan and Glassman, Sydney I. and Helm, Aveliina and Henry, Hugh A. L. and H{\'e}rault, Bruno and Hiiesalu, Indrek and Hiiesalu, Inga and Hozzein, Wael N. and Kohout, Petr and K{\~o}ljalg, Urmas and Koorem, Kadri and Laanisto, Lauri and Mander, {\"U}lo and Mucina, Ladislav and Munyampundu, Jean-Pierre and Neuenkamp, Lena and Niinemets, {\"U}lo and Nyamukondiwa, Casper and Oja, Jane and Onipchenko, Vladimir and P{\"a}rtel, Meelis and Phosri, Cherdchai and P{\~o}lme, Sergei and P{\"u}ssa, Kersti and Ronk, Argo and Saitta, Alessandro and Semboli, Olivia and Sepp, Siim-Kaarel and Seregin, Alexey and Sudheer, Surya and {Pe{\~n}a-Venegas}, Clara P. and Paz, Claudia and Vahter, Tanel and Vasar, Martti and Veraart, Annelies J. and Tedersoo, Leho and Zobel, Martin and {\"O}pik, Maarja},
  year = {2021},
  journal = {New Phytologist},
  volume = {231},
  number = {2},
  pages = {763--776},
  issn = {1469-8137},
  doi = {10.1111/nph.17240},
  urldate = {2023-08-02},
  abstract = {The arbuscular mycorrhizal (AM) fungi are a globally distributed group of soil organisms that play critical roles in ecosystem function. However, the ecological niches of individual AM fungal taxa are poorly understood. We collected {$>$} 300 soil samples from natural ecosystems worldwide and modelled the realised niches of AM fungal virtual taxa (VT; approximately species-level phylogroups). We found that environmental and spatial variables jointly explained VT distribution worldwide, with temperature and pH being the most important abiotic drivers, and spatial effects generally occurring at local to regional scales. While dispersal limitation could explain some variation in VT distribution, VT relative abundance was almost exclusively driven by environmental variables. Several environmental and spatial effects on VT distribution and relative abundance were correlated with phylogeny, indicating that closely related VT exhibit similar niche optima and widths. Major clades within the Glomeraceae exhibited distinct niche optima, Acaulosporaceae generally had niche optima in low pH and low temperature conditions, and Gigasporaceae generally had niche optima in high precipitation conditions. Identification of the realised niche space occupied by individual and phylogenetic groups of soil microbial taxa provides a basis for building detailed hypotheses about how soil communities respond to gradients and manipulation in ecosystems worldwide.},
  copyright = {{\copyright} 2021 The Authors New Phytologist {\copyright} 2021 New Phytologist Foundation},
  langid = {english},
  keywords = {arbuscular mycorrhizal fungi,ecological niche,molecular taxa,niche optimum,niche width,pH,phylogenetic correlation,temperature},
  file = {/home/brfurnea/gdrive/Papers/Fungal communities/AM communities/Davison_et_al_2021_Temperature_and_pH_define_the_realised_niche_space_of_arbuscular_mycorrhizal.pdf}
}
