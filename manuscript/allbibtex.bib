@article{rahn2018,
  title = {Generic Accelerated Sequence Alignment in {{SeqAn}} Using Vectorization and Multi-Threading},
  author = {Rahn, Ren{\'e} and Budach, Stefan and Costanza, Pascal and Ehrhardt, Marcel and Hancox, Jonny and Reinert, Knut},
  year = {2018},
  month = oct,
  journal = {Bioinformatics},
