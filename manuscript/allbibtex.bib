@article{kyaschenko2017,
  title = {Below-Ground Organic Matter Accumulation along a Boreal Forest Fertility Gradient Relates to Guild Interaction within Fungal Communities},
  author = {Kyaschenko, Julia and Clemmensen, Karina E. and Karltun, Erik and Lindahl, Bj{\"o}rn D.},
