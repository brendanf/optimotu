@article{lanzen2012,
  title = {{{CREST}} -- {{Classification Resources}} for {{Environmental Sequence Tags}}},
  author = {Lanz{\'e}n, Anders and J{\o}rgensen, Steffen L. and Huson, Daniel H. and Gorfer, Markus and Grindhaug, Svenn Helge and Jonassen, Inge and {\O}vre{\aa}s, Lise and Urich, Tim},
  year = {2012},
