@article{meyer2007,
  title = {{{SimulFold}}: {{Simultaneously Inferring RNA Structures Including Pseudoknots}}, {{Alignments}}, and {{Trees Using}} a {{Bayesian MCMC Framework}}},
  shorttitle = {{{SimulFold}}},
  author = {Meyer, Irmtraud M and Mikl{\'o}s, Istv{\'a}n},
