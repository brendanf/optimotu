@article{arora2014,
  title = {Clarifying the Butter {{Boletes}}: A New Genus, {{Butyriboletus}}, Is Established to Accommodate {{Boletus}} Sect. {{Appendiculati}}, and Six New Species Are Described},
  shorttitle = {Clarifying the Butter {{Boletes}}},
  author = {Arora, David and Frank, Jonathan L.},
