@article{aignon2021,
  title = {Mallocybe Africana ({{Inocybaceae}}, {{Fungi}}), the First Species of {{Mallocybe}} Described from {{Africa}}},
  author = {A{\"i}gnon, Hyppolite L. and Naseer, Arooj and Matheny, Brandon P. and Yorou, Nourou S. and Ryberg, Martin},
