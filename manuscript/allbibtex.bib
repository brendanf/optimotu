@article{ficetola2017,
  title = {Global Determinants of Zoogeographical Boundaries},
  author = {Ficetola, Gentile Francesco and Mazel, Florent and Thuiller, Wilfried},
  year = {2017},
  month = mar,
  journal = {Nature Ecology \& Evolution},
