@article{golan2019,
  title = {Native and Invasive Populations of the Ectomycorrhizal Death Cap {{Amanita}} Phalloides Are Highly Sexual but Dispersal Limited},
  author = {Golan, Jacob and Adams, Catherine A. and Cross, Hugh and Elmore, Holly and Gardes, Monique and Glassman, Sydney I. and Gon{\c c}alves, Susana C. and Hess, Jaqueline and Richard, Franck and Wang, Yen-Wen and Wolfe, Benjamin and Pringle, Anne},
  year = {2019},
