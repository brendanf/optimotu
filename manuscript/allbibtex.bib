  langid = {english},
  keywords = {African woodland,Agaricales,Fungi,morphology,systematics,taxonomy},
  file = {/home/brfurnea/gdrive/Papers/Systematics/Fungi/Basidiomycota/Inocybe/Aignon_etal2021Mallocybe africana (Inocybaceae, Fungi), the first species of Mallocybe(Phytotaxa).pdf;/home/brfurnea/Zotero/storage/KWR9IDUW/phytotaxa.478.1.html}
