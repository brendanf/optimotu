  keywords = {Archaeosporales,Glomeromycota,long-read metabarcoding,mycorrhizal fungi distribution,PacBio,third-generation sequencing},
  file = {/home/brfurnea/gdrive/Papers/Lab Methods/Molecular Biology/PacBio/Kolaříková_et_al_2021_PacBio_sequencing_of_Glomeromycota_rDNA.pdf;/home/brfurnea/Zotero/storage/B32LC8UN/nph.html}
