@article{looney,
  title = {Evolutionary Transition to the Ectomycorrhizal Habit in the Genomes of a Hyper-Diverse Lineage of Mushroom-Forming Fungi},
  author = {Looney, Brian and Miyauchi, Shingo and Morin, Emmanuelle and Drula, Elodie and Courty, Pierre Emmanuel and Kohler, Annegret and Kuo, Alan and LaButti, Kurt and Pangilinan, Jasmyn and Lipzen, Anna and Riley, Robert and Andreopoulos, William and He, Guifen and Johnson, Jenifer and Nolan, Matt and Tritt, Andrew and Barry, Kerrie W. and Grigoriev, Igor V. and Nagy, L{\'a}szl{\'o} G. and Hibbett, David and Henrissat, Bernard and Matheny, P. Brandon and Labb{\'e}, Jesse and Martin, Francis M.},
  journal = {New Phytologist},
  volume = {n/a},
  number = {n/a},
