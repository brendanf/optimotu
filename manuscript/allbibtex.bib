@article{khmelnitsky2019,
  title = {Ionosporus: A New Genus for {{Boletus}} Longipes ({{Boletaceae}}), with a New Species, {{I}}. Australis, from {{Australia}}},
  shorttitle = {Ionosporus},
  author = {Khmelnitsky, Olga and Davoodian, Naveed and Singh, Pooja and Rasp{\'e}, Olivier and Lee, Serena M.L. and Fechner, Nigel and Bonito, Gregory and Lebel, Teresa and Halling, Roy E.},
