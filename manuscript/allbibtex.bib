  title = {Interspecific {{Mycorrhizal Networks}} and {{Non-networking Hosts}}: {{Exploring}} the {{Ecology}} of the {{Host Genus Alnus}}},
  shorttitle = {Interspecific {{Mycorrhizal Networks}} and {{Non-networking Hosts}}},
