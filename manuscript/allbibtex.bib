@article{riaz2011,
  title = {{{ecoPrimers}}: Inference of New {{DNA}} Barcode Markers from Whole Genome Sequence Analysis},
  shorttitle = {{{ecoPrimers}}},
  author = {Riaz, Tiayyba and Shehzad, Wasim and Viari, Alain and Pompanon, Fran{\c c}ois and Taberlet, Pierre and Coissac, Eric},
