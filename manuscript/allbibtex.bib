@article{puente-sanchez2016,
  title = {A Novel Conceptual Approach to Read-Filtering in High-Throughput Amplicon Sequencing Studies},
  author = {{Puente-S{\'a}nchez}, Fernando and Aguirre, Jacobo and Parro, V{\'i}ctor},
  year = {2016},
