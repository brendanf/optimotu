@article{marco-sola2023,
  title = {Optimal Gap-Affine Alignment in {{O}}(s) Space},
  author = {{Marco-Sola}, Santiago and Eizenga, Jordan M and Guarracino, Andrea and Paten, Benedict and Garrison, Erik and Moreto, Miquel},
  year = {2023},
  month = feb,
