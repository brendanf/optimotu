@article{farid2020,
  title = {Expansion of the Genus {{Imleria}} in {{North America}} to Include {{Imleria}} Floridana, Sp. Nov., and {{Imleria}} Pallida, Comb. Nov.},
  author = {Farid, Arian and Franck, Alan R. and Bolin, Jason and Garey, James R.},
