@article{yorou2001,
  title = {Connaissances Ethnomycologiques Des Peuples {{Nagot}} Du Centre Du {{B{\'e}nin}} ({{Afrique}} de l'{{Ouest}})},
  author = {Yorou, Nourou S. and De Kesel, Andr{\'e}},
  year = {2001},
  journal = {Systematics and Geography of Plants},
  volume = {71},
  number = {2},
  eprint = {3668707},
  eprinttype = {jstor},
  pages = {627--637},
  publisher = {National Botanic Garden of Belgium},
  issn = {1374-7886},
  doi = {10.2307/3668707},
  urldate = {2020-08-11},
  abstract = {An ethnomycological study was undertaken amongst the Nagot people from central B{\'e}nin (West Africa). The results show that the ethnomycological knowledge from the Nagot people are through and rich. A list of nearly fifty edible macromycetes with their corresponding Nagot vernacular name and meaning, is given. Aspects concerning the local sources of information, the transmission of indigenous knowledge, attitudes, the role of edible fungi in diets, the importance of mushroom picking and the effect of gender, are treated. The way Nagot people delimit mushroom species, make vernacular names and recognize toxic species is discussed. /// Une {\'e}tude ethnomycologique a {\'e}t{\'e} r{\'e}alis{\'e}e aupr{\`e}s de la population Nagot du centre du B{\'e}nin (Afrique de l'Ouest). L'{\'e}tude montre que les connaissances ethnomycologiques des Nagot sont profondes et riches. Une cinquantaine d'esp{\`e}ces comestibles sont r{\'e}pertori{\'e}es, leur(s) nom(s) en Nagot et la signification sont donn{\'e}s. Les aspects li{\'e}s aux sources d'information, la transmission des connaissances endog{\`e}nes, l'attitude g{\'e}n{\'e}rale des Nagot, le r{\^o}le des champignons sauvages dans l'alimentation, l'importance de la cueillette, le savoir et l'impact du sexe, sont trait{\'e}s. La notion d'esp{\`e}ce chez les Nagot, la fa{\c c}on de former les noms vernaculaires et de faire la distinction entre esp{\`e}ces dangereuses et comestibles, est discut{\'e}e.}
}
