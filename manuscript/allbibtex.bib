@article{visagie2014,
  title = {Identification and Nomenclature of the Genus {{Penicillium}}},
  author = {Visagie, C.M. and Houbraken, J. and Frisvad, J.C. and Hong, S.-B. and Klaassen, C.H.W. and Perrone, G. and Seifert, K.A. and Varga, J. and Yaguchi, T. and Samson, R.A.},
  year = {2014},
