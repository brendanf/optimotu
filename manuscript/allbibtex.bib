@article{webb2000,
  title = {Exploring the {{Phylogenetic Structure}} of {{Ecological Communities}}: {{An Example}} for {{Rain Forest Trees}}.},
  shorttitle = {Exploring the {{Phylogenetic Structure}} of {{Ecological Communities}}},
  author = {Webb, Campbell O.},
