@article{rossman2013,
  title = {Genera in {{Bionectriaceae}}, {{Hypocreaceae}}, and {{Nectriaceae}} ({{Hypocreales}}) Proposed for Acceptance or Rejection},
  author = {Rossman, Amy Y. and Seifert, Keith A. and Samuels, Gary J. and Minnis, Andrew M. and Schroers, Hans-Josef and Lombard, Lorenzo and Crous, Pedro W. and P{\~o}ldmaa, Kadri and Cannon, Paul F. and Summerbell, Richard C. and Geiser, David M. and Zhuang, Wen-ying and Hirooka, Yuuri and Herrera, Cesar and {Salgado-Salazar}, Catalina and Chaverri, Priscila},
  year = {2013},
