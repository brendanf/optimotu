@article{eshghisahraei2022,
  title = {Effects of Operational Taxonomic Unit Inference Methods on Soil Microeukaryote Community Analysis Using Long-Read Metabarcoding},
  author = {Eshghi Sahraei, Shadi and Furneaux, Brendan and Kluting, Kerri and Zakieh, Mustafa and Rydin, H{\aa}kan and Hytteborn, H{\aa}kan and Rosling, Anna},
