  title = {Long- and Short-Read Metabarcoding Technologies Reveal Similar Spatiotemporal Structures in Fungal Communities},
  author = {Furneaux, Brendan and Bahram, Mohammad and Rosling, Anna and Yorou, Nourou S. and Ryberg, Martin},
