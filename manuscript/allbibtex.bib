@article{mossebo2017,
  title = {Phylogenetic Relationships, Taxonomic Revision and New Taxa of {{Termitomyces}} ({{Lyophyllaceae}}, {{Basidiomycota}}) Inferred from Combined {{nLSU-}} and {{mtSSU-rDNA}} Sequences},
  author = {Mossebo, D. C. and Essouman, E. P. F. and Machouart, M. C. and Gueidan, C.},
