@article{yamada2016,
  title = {Application of the {{MAFFT}} Sequence Alignment Program to Large Data---Reexamination of the Usefulness of Chained Guide Trees},
  author = {Yamada, Kazunori D. and Tomii, Kentaro and Katoh, Kazutaka},
