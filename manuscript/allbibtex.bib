  title = {Diversity, Phenology and Distribution of {{Termitomyces}} Species in {{C{\^o}te}} d'{{Ivoire}}},
  author = {Kon{\'e}, N'golo Abdoulaye and Soro, Bakary and {Vani{\'e}-L{\'e}abo}, Linda Patricia Louyounan and Konat{\'e}, Souleymane and Bakayoko, Adama and Kon{\'e}, Daouda},
