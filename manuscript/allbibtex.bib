@article{lesage-meessen2011,
  title = {Phylogeographic Relationships in the Polypore Fungus {{Pycnoporus}} Inferred from Molecular Data},
  author = {{Lesage-Meessen}, Laurence and Haon, Mireille and Uzan, Eva and Levasseur, Anthony and Piumi, Fran{\c c}ois and Navarro, David and Taussac, Sabine and Favel, Anne and Lomascolo, Anne},
  year = {2011},
