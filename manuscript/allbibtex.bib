@article{alser2020,
  title = {{{SneakySnake}}: A Fast and Accurate Universal Genome Pre-Alignment Filter for {{CPUs}}, {{GPUs}} and {{FPGAs}}},
  shorttitle = {{{SneakySnake}}},
  author = {Alser, Mohammed and Shahroodi, Taha and {G{\'o}mez-Luna}, Juan and Alkan, Can and Mutlu, Onur},
  year = {2020},
  month = dec,
  journal = {Bioinformatics},
  volume = {36},
  number = {22-23},
  pages = {5282--5290},
  issn = {1367-4803},
  doi = {10.1093/bioinformatics/btaa1015},
  urldate = {2023-04-03},
  abstract = {We introduce SneakySnake, a highly parallel and highly accurate pre-alignment filter that remarkably reduces the need for computationally costly sequence alignment. The key idea of SneakySnake is to reduce the approximate string matching (ASM) problem to the single net routing (SNR) problem in VLSI chip layout. In the SNR problem, we are interested in finding the optimal path that connects two terminals with the least routing cost on a special grid layout that contains obstacles. The SneakySnake algorithm quickly solves the SNR problem and uses the found optimal path to decide whether or not performing sequence alignment is necessary. Reducing the ASM problem into SNR also makes SneakySnake efficient to implement on CPUs, GPUs and FPGAs.SneakySnake significantly improves the accuracy of pre-alignment filtering by up to four orders of magnitude compared to the state-of-the-art pre-alignment filters, Shouji, GateKeeper and SHD. For short sequences, SneakySnake accelerates Edlib (state-of-the-art implementation of Myers's bit-vector algorithm) and Parasail (state-of-the-art sequence aligner with a configurable scoring function), by up to 37.7{\texttimes} and 43.9{\texttimes} (\&gt;12{\texttimes} on average), respectively, with its CPU implementation, and by up to 413{\texttimes} and 689{\texttimes} (\&gt;400{\texttimes} on average), respectively, with FPGA and GPU acceleration. For long sequences, the CPU implementation of SneakySnake accelerates Parasail and KSW2 (sequence aligner of minimap2) by up to 979{\texttimes} (276.9{\texttimes} on average) and 91.7{\texttimes} (31.7{\texttimes} on average), respectively. As SneakySnake does not replace sequence alignment, users can still obtain all capabilities (e.g. configurable scoring functions) of the aligner of their choice, unlike existing acceleration efforts that sacrifice some aligner capabilities.https://github.com/CMU-SAFARI/SneakySnake.Supplementary data are available at Bioinformatics online.},
  file = {/home/brfurnea/gdrive/Papers/Computer Methods/Bioinformatics/Alignment/Alser_et_al_2020_SneakySnake.pdf;/home/brfurnea/Zotero/storage/ZW2N7W3A/6033580.html}
}
