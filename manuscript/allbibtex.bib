@article{gessner1993,
  title = {Ergosterol-to-{{Biomass Conversion Factors}} for {{Aquatic Hyphomycetes}}},
  author = {Gessner, Mark O. and Chauvet, Eric},
  year = {1993},
  month = feb,
  journal = {Applied and Environmental Microbiology},
  volume = {59},
