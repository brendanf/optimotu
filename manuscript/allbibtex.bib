@article{ng2000,
  title = {{{PHAT}}: A Transmembrane-Specific Substitution Matrix. {{Predicted}} Hydrophobic and Transmembrane},
  shorttitle = {{{PHAT}}},
  author = {Ng, P. C. and Henikoff, J. G. and Henikoff, S.},
  year = {2000},
