@article{zinger2009,
  title = {Assessment of {{Microbial Communities}} by {{Graph Partitioning}} in a {{Study}} of {{Soil Fungi}} in {{Two Alpine Meadows}}},
  author = {Zinger, L. and Coissac, E. and Choler, P. and Geremia, R. A.},
