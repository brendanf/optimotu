@article{taylor2014,
  title = {A First Comprehensive Census of Fungi in Soil Reveals Both Hyperdiversity and Fine-Scale Niche Partitioning},
  author = {Taylor, D. Lee and Hollingsworth, Teresa N. and McFarland, Jack W. and Lennon, Niall J. and Nusbaum, Chad and Ruess, Roger W.},
