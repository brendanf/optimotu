@article{eberhardt2002,
  title = {Molecular Kinship Analyses of the Agaricoid {{Russulaceae}}: {{Correspondence}} with Mycorrhizal Anatomy and Sporocarp Features in the Genus {{{\emph{Russula}}}}},
