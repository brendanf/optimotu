  title = {Aligning {{Goals}}, {{Assessments}}, and {{Activities}}: {{An Approach}} to {{Teaching PCR}} and {{Gel Electrophoresis}}},
  shorttitle = {Aligning {{Goals}}, {{Assessments}}, and {{Activities}}},
  author = {Phillips, Allison R. and Robertson, Amber L. and Batzli, Janet and Harris, Michelle and Miller, Sarah},
