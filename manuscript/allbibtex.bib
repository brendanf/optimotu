@article{leeSurfaceWaterNutrient2002,
  title = {Surface Water Nutrient Concentrations and Litter Decomposition Rates in Wetlands Impacted by Agriculture and Mining Activities},
  author = {Lee, A. A and Bukaveckas, P. A},
  year = {2002},
