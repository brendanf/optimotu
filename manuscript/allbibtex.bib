@article{garnica2016,
  title = {Divergence {{Times}} and {{Phylogenetic Patterns}} of {{Sebacinales}}, a {{Highly Diverse}} and {{Widespread Fungal Lineage}}},
  author = {Garnica, Sigisfredo and Riess, Kai and Sch{\"o}n, Max E. and Oberwinkler, Franz and Setaro, Sabrina D.},
