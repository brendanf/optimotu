@article{zheng2019,
  title = {A Parallel Computational Framework for Ultra-Large-Scale Sequence Clustering Analysis},
  author = {Zheng, Wei and Mao, Qi and Genco, Robert J and {Wactawski-Wende}, Jean and Buck, Michael and Cai, Yunpeng and Sun, Yijun},
