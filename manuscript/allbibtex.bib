@article{may2014,
  title = {Unraveling the Outcome of {{16S rDNA-based}} Taxonomy Analysis through Mock Data and Simulations},
  author = {May, Ali and Abeln, Sanne and Crielaard, Wim and Heringa, Jaap and Brandt, Bernd W.},
