  shorttitle = {{{WFA-FPGA}}},
  author = {Haghi, Abbas and {Marco-Sola}, Santiago and Alvarez, Lluc and Diamantopoulos, Dionysios and Hagleitner, Christoph and Moreto, Miquel},
