@misc{edgar2021,
  title = {{{MUSCLE}} v5 Enables Improved Estimates of Phylogenetic Tree Confidence by Ensemble Bootstrapping},
  author = {Edgar, Robert C.},
  year = {2021},
  month = jun,
  primaryclass = {New Results},
