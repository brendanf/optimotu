@book{tedersoo2022a,
  title = {{{MycoPhylo}} Experiment Reveals How Mycorrhiza Types and Phylogenetic Relationships Affect Soil Biodiversity and Functioning},
  author = {Tedersoo, Leho and Loit, Kaire and Agan, Ahto and Rahimlou, Saleh and Vask, Annaliisa and Drenkhan, Rein},
  year = {2022},
  month = apr,
  doi = {10.1101/2022.04.26.489578},
  abstract = {Natural forests and abandoned agricultural lands are increasingly replaced by monospecific forest plantations that have poor capacity to support biodiversity and ecosystem services. Natural forests harbour plants belonging to different mycorrhiza types that differ in their microbiome and carbon and nutrient cycling properties. Here we describe the MycoPhylo field experiment that encompasses 116 woody plant species from three mycorrhiza types and 237 plots, with plant diversity and mycorrhiza type diversity ranging from one to four and one to three per plot, respectively. The MycoPhylo experiment enables us to test hypotheses about the plant species, species diversity, mycorrhiza type, and mycorrhiza type diversity effects and their phylogenetic context on soil microbial diversity and functioning and soil processes. Alongside with other experiments in the TreeDivNet consortium, MycoPhylo will contribute to our understanding of the tree diversity effects on soil biodiversity and ecosystem functioning across biomes, especially from the mycorrhiza type and phylogenetic conservatism perspectives.},
  file = {/home/brfurnea/gdrive/Papers/Ecology/Community Phylogenetics/Tedersoo_et_al_2022_MycoPhylo_experiment_reveals_how_mycorrhiza_types_and_phylogenetic.pdf}
}
