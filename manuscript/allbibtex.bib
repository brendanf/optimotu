@article{ogawa2019,
  title = {Efficient Establishment of Pure Cultures of Yellow Chanterelle {{Cantharellus}} Anzutake from Ectomycorrhizal Root Tips, and Morphological Characteristics of Ectomycorrhizae and Cultured Mycelium},
  author = {Ogawa, Wakana and Endo, Naoki and Takeda, Yumi and Kodaira, Miyuki and Fukuda, Masaki and Yamada, Akiyoshi},
  year = {2019},
  month = jan,
  journal = {Mycoscience},
  volume = {60},
  number = {1},
  pages = {45--53},
