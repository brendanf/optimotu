@article{bailly2007,
  title = {Soil Eukaryotic Functional Diversity, a Metatranscriptomic Approach},
  author = {Bailly, Julie and {Fraissinet-Tachet}, Laurence and Verner, Marie-Christine and Debaud, Jean-Claude and Lemaire, Marc and {W{\'e}solowski-Louvel}, Micheline and Marmeisse, Roland},
  year = {2007},
