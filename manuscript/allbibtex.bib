@article{vanderheijden2015,
  title = {Mycorrhizal Ecology and Evolution: The Past, the Present, and the Future},
  shorttitle = {Mycorrhizal Ecology and Evolution},
  author = {{\noopsort{heijden}}{van der Heijden}, Marcel G. A. and Martin, Francis M. and Selosse, Marc-Andr{\'e} and Sanders, Ian R.},
