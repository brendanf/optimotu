  title = {Preferential {{PCR Amplification}} of {{Parasitic Protistan Small Subunit rDNA}} from {{Metazoan Tissues}}},
  author = {Bower, Susan M. and Carnegie, Ryan B. and Goh, Benjamin and Jones, Simon R. M. and Lowe, Geoffrey J. and Mak, Michelle W. S.},
  year = {2004},
