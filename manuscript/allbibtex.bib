  langid = {english},
  keywords = {Algae,amoebae,biodiversity,ciliate,ecology,flagellate,fungus,microbiology,parasite,plankton,protozoa,systematics,taxonomy},
  file = {/home/brfurnea/gdrive/Papers/Systematics/Adl_etal_2019_Revisions_to_the_Classification,_Nomenclature,_and_Diversity_of_Eukaryotes(Journal_of_Eukaryotic_Microbiology).pdf;/home/brfurnea/Zotero/storage/QLXVQBB9/jeu.html}
