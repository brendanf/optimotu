@article{rosling2011,
  title = {Archaeorhizomycetes: {{Unearthing}} an {{Ancient Class}} of {{Ubiquitous Soil Fungi}}},
  shorttitle = {Archaeorhizomycetes},
  author = {Rosling, Anna and Cox, Filipa and {Cruz-Martinez}, Karelyn and Ihrmark, Katarina and Grelet, Gwen-A{\"e}lle and Lindahl, Bj{\"o}rn D. and Menkis, Audrius and James, Timothy Y.},
