@article{banchi2020,
  title = {{{PLANiTS}}: A Curated Sequence Reference Dataset for Plant {{ITS DNA}} Metabarcoding},
  shorttitle = {{{PLANiTS}}},
  author = {Banchi, Elisa and Ametrano, Claudio G and Greco, Samuele and Stankovi{\'c}, David and Muggia, Lucia and Pallavicini, Alberto},
