@article{boke-olen2016,
  title = {Estimating and {{Analyzing Savannah Phenology}} with a {{Lagged Time Series Model}}},
  author = {{Boke-Ol{\'e}n}, Niklas and Lehsten, Veiko and Ard{\"o}, Jonas and Beringer, Jason and Eklundh, Lars and Holst, Thomas and Veenendaal, Elmar and Tagesson, Torbern},
  year = {2016},
