@article{yu2021,
  title = {Using {{Robinson-Foulds}} Supertrees in Divide-and-Conquer Phylogeny Estimation},
  author = {Yu, Xilin and Le, Thien and Christensen, Sarah A. and Molloy, Erin K. and Warnow, Tandy},
