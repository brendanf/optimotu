@article{elbrecht2019,
  title = {Validation of {{COI}} Metabarcoding Primers for Terrestrial Arthropods},
  author = {Elbrecht, Vasco and Braukmann, Thomas W. A. and Ivanova, Natalia V. and Prosser, Sean W. J. and Hajibabaei, Mehrdad and Wright, Michael and Zakharov, Evgeny V. and Hebert, Paul D. N. and Steinke, Dirk},
  year = {2019},
