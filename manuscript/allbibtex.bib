@article{shchepin2019,
  title = {Community of Dark-Spored Myxomycetes in Ground Litter and Soil of Taiga Forest ({{Nizhne-Svirskiy Reserve}}, {{Russia}}) Revealed by {{DNA}} Metabarcoding},
