@article{kuske2015,
  title = {Prospects and Challenges for Fungal Metatranscriptomics of Complex Communities},
  author = {Kuske, Cheryl R. and Hesse, Cedar N. and Challacombe, Jean F. and Cullen, Daniel and Herr, Joshua R. and Mueller, Rebecca C. and Tsang, Adrian and Vilgalys, Rytas},
