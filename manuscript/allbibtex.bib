@article{burki2007,
  title = {Phylogenomics {{Reshuffles}} the {{Eukaryotic Supergroups}}},
  author = {Burki, Fabien and {Shalchian-Tabrizi}, Kamran and Minge, Marianne and Skj{\ae}veland, {\AA}smund and Nikolaev, Sergey I. and Jakobsen, Kjetill S. and Pawlowski, Jan},
  year = {2007},
  month = aug,
  journal = {PLOS ONE},
  volume = {2},
  number = {8},
  pages = {e790},
  publisher = {Public Library of Science},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0000790},
  urldate = {2020-07-23},
  abstract = {BackgroundResolving the phylogenetic relationships between eukaryotes is an ongoing challenge of evolutionary biology. In recent years, the accumulation of molecular data led to a new evolutionary understanding, in which all eukaryotic diversity has been classified into five or six supergroups. Yet, the composition of these large assemblages and their relationships remain controversial.Methodology/Principle FindingsHere, we report the sequencing of expressed sequence tags (ESTs) for two species belonging to the supergroup Rhizaria and present the analysis of a unique dataset combining 29908 amino acid positions and an extensive taxa sampling made of 49 mainly unicellular species representative of all supergroups. Our results show a very robust relationship between Rhizaria and two main clades of the supergroup chromalveolates: stramenopiles and alveolates. We confirm the existence of consistent affinities between assemblages that were thought to belong to different supergroups of eukaryotes, thus not sharing a close evolutionary history.ConclusionsThis well supported phylogeny has important consequences for our understanding of the evolutionary history of eukaryotes. In particular, it questions a single red algal origin of the chlorophyll-c containing plastids among the chromalveolates. We propose the abbreviated name `SAR' (Stramenopiles+Alveolates+Rhizaria) to accommodate this new super assemblage of eukaryotes, which comprises the largest diversity of unicellular eukaryotes.},
  langid = {english},
  keywords = {Algae,Eukaryota,Eukaryotic evolution,Evolutionary biology,Phylogenetic analysis,Phylogenetics,Plastids,Sequence alignment},
  file = {/home/brfurnea/gdrive/Papers/Systematics/Eukarya/Burki_etal2007Phylogenomics Reshuffles the Eukaryotic Supergroups(PLOS ONE).pdf;/home/brfurnea/Zotero/storage/DSIX2S4A/article.html}
}
