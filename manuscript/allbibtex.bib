@article{damon2012,
  title = {Metatranscriptomics {{Reveals}} the {{Diversity}} of {{Genes Expressed}} by {{Eukaryotes}} in {{Forest Soils}}},
  author = {Damon, Coralie and Lehembre, Fr{\'e}d{\'e}ric and {Oger-Desfeux}, Christine and Luis, Patricia and Ranger, Jacques and {Fraissinet-Tachet}, Laurence and Marmeisse, Roland},
  year = {2012},
