@article{haelewaters2020,
  title = {Fungal {{Systematics}} and {{Evolution}}: {{FUSE}} 6},
  shorttitle = {Fungal {{Systematics}} and {{Evolution}}},
  author = {Haelewaters, Danny and Dima, B{\'a}lint and {Abdel-Hafiz}, Abbas I. I. and {Abdel-Wahab}, Mohamed A. and {Abul-Ezz}, Samar R. and Acar, Ismail and Nagy, Angelina and Papp, Viktor and P{\'e}ter, {\'A}ron and Pfliegler, Walter P. and S{\'a}ndor, Attila D. and Szentiv{\'a}nyi, Tamara and {Krisai-Greilhuber}, Irmgard},
  year = {2020},
