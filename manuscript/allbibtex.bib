@article{li2024,
  title = {{{PROTAX-GPU}}: A Scalable Probabilistic Taxonomic Classification System for {{DNA}} Barcodes},
  shorttitle = {{{PROTAX-GPU}}},
  author = {Li, Roy and Ratnasingham, Sujeevan and Zarubiieva, Iuliia and Somervuo, Panu and Taylor, Graham W.},
