@article{shirouzu2020,
  title = {Phylogenetic Overview of {{Erysiphaceae}} Based on {{nrDNA}} and {{MCM7}} Sequences},
  author = {Shirouzu, Takashi and Takamatsu, Susumu and Hashimoto, Akira and Meeboon, Jamjan and Ohkuma, Moriya},
  year = {2020},
  month = sep,
  journal = {Mycoscience},
