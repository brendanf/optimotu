@article{suzuki2020,
  title = {Evaluation of Primer Pairs for Studying Arbuscular Mycorrhizal Fungal Community Compositions Using a {{MiSeq}} Platform},
  author = {Suzuki, Kazuki and Takahashi, Kohei and Harada, Naoki},
  year = {2020},
