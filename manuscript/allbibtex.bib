@article{yorou2001a,
  ids = {Yorou2001},
  title = {Diversit{\'e} et Productivit{\'e} Des Champignons Comestibles de La For{\^e}t Class{\'e}e de {{Wari-Maro}} ({{B{\'e}nin}}, {{Afrique}} de l'{{Ouest}})},
  author = {Yorou, Nourou S. and De Kesel, Andr{\'e} and Sinsin, B. and Codjia, J. T. C.},
  year = {2001},
  journal = {Systematics and Geography of Plants},
  volume = {71},
  number = {2},
  eprint = {3668706},
  eprinttype = {jstor},
  pages = {613--625},
  publisher = {JSTOR},
  issn = {1374-7886},
  doi = {10.2307/3668706},
  urldate = {2020-08-17},
  abstract = {This paper presents a preliminary inventory of edible macromycetes found in six Sudano-Guinean vegetation types from the protected forest areas of Wari-Maro in Benin (West Africa). Thirty edible species were found in 18 fixed plots (three per vegetation type). Edibility data were obtained from local populations and the literature. Mushroom productions were assessed per species and per plot using the number of produced carpophores and fresh weight (kg/ha). Edible macromycetes from the studied areas are for 86.6\% ectomycorrhizal taxa belonging to Lactarius, Russula and Amanita. The total macromycetous fresh weight production of one season varies from 0.2 to 225.0 kg/ha, depending on the forest type. Open forests, i.e. savanna and woodland savanna, dominated by Caesalpiniaceae yield qualitatively and quantitatively more edible taxa than the denser and rivulet associated or riverain forest types lacking Caesalpinioid trees. Lactarius gymnocarpoides, forming ectomycorrhiza with Isoberlinia doka, is the most common and abundant species in the savannas and woodland savannas. Specific productions of the edible taxa are affected by host tree composition reflected in the forest type. Most species produce quantitatively more fresh weight in the first two months of the rainy season (June-July). Species showing a continuous production throughout the rainy season are rare. The study illustrates that the edible macromycetes from Wari-Maro forest have substantial importance as a renewable non timber forest product (NTFP). The limitations and the main factors, necessary for a succesful valorisation of these NTFP, are discussed. /// Cette {\'e}tude donne l'inventaire des macromyc{\`e}tes comestibles, trouv{\'e}s dans six types de formations foresti{\`e}res de la for{\^e}t class{\'e}e de Wari-Maro (B{\'e}nin, Afrique de l'Ouest) en r{\'e}gion Soudano-guin{\'e}enne. Un total de 30 esp{\`e}ces comestibles a {\'e}t{\'e} obtenu dans 18 placeaux (trois par formation), les donn{\'e}es de comestibilit{\'e} ont {\'e}t{\'e} obtenues de la population locale et de la litt{\'e}rature. Le nombre de carpophores et la biomasse fra{\^i}che ont {\'e}t{\'e} quantifi{\'e}s par esp{\`e}ce et par formation. Les macromyc{\`e}tes comestibles sont {\`a} 86,6\% des esp{\`e}ces ectomycorhiziennes appartenant principalement aux genres Amanita, Lactarius et Russula. En une saison la production totale, toutes esp{\`e}ces confondues, varie de 0,2 {\`a} 225 kg MF/ha suivant la formation foresti{\`e}re. Les formations ouvertes (savane arbustive, savane arbor{\'e}e et les diff{\'e}rents faci{\`e}s de la for{\^e}t claire {\`a} Isoberlinia doka) domin{\'e}es par les Caesalpiniaceae fournissent le plus d'esp{\`e}ces comestibles, tant en richesse sp{\'e}cifique qu'en biomasse fra{\^i}che. Par contre, les formations denses ferm{\'e}es (for{\^e}t dense de plateau et for{\^e}t riveraine) pauvres en Caesalpiniaceae enregistrent les plus faibles productions. Lactarius gymnocarpoides, esp{\`e}ce symbiote d'Isoberlinia doka, est la plus abondante de la mycoflore des formations {\`a} caract{\`e}re de for{\^e}t claire. Les productions sp{\'e}cifiques varient d'une formation {\`a} une autre. Quelques cas sp{\'e}cifiques sont trait{\'e}s. Les productions des esp{\`e}ces sont le plus souvent concentr{\'e}es en d{\'e}but de la saison des pluies (juin-juillet). Les esp{\`e}ces {\`a} production plus {\'e}tal{\'e}e sont peu fr{\'e}quentes. L'{\'e}tude d{\'e}montre que les potentialit{\'e}s des macromyc{\`e}tes de la for{\^e}t class{\'e}e de Wari-Maro, en tant que ressource foresti{\`e}re comestible ou produit forestier non-ligneux (PFNL), sont {\'e}normes. Les facteurs importants pour le succ{\`e}s de la valorisation des ressources mycologiques dans la r{\'e}gion sont abord{\'e}s.},
  keywords = {Benin},
  file = {/home/brfurnea/gdrive/Papers/Fungal communities/Yorou_etal_2001_Diversité_et_productivité_des_champignons_comestibles_de_la_forêt_classée_de(Systematics_and_Geography_of_Plants).pdf;/home/brfurnea/gdrive/Papers/Fungal communities/Yorou_etal_2001_Diversité_et_productivité_des_champignons_comestibles_de_la_forêt_classée_de(Systematics_and_Geography_of_Plants).pdf}
}
