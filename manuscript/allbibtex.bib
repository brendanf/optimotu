@article{aignon2021a,
  title = {Three New Species of {{Inosperma}} ({{Agaricales}}, {{Inocybaceae}}) from {{Tropical Africa}}},
  author = {A{\"i}gnon, Hyppolite L. and Jabeen, Sana and Naseer, Arooj and Yorou, Nourou S. and Ryberg, Martin},
  year = {2021},
