@article{macher2021,
  title = {{{TaxonTableTools}}: {{A}} Comprehensive, Platform-Independent Graphical User Interface Software to Explore and Visualise {{DNA}} Metabarcoding Data},
  shorttitle = {{{TaxonTableTools}}},
