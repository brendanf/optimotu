@article{klamer2004,
  title = {Estimation of Conversion Factors for Fungal Biomass Determination in Compost Using Ergosterol and {{PLFA}} 18:2{$\omega$}6,9},
  shorttitle = {Estimation of Conversion Factors for Fungal Biomass Determination in Compost Using Ergosterol and {{PLFA}} 18},
  author = {Klamer, Morten and B{\aa}{\aa}th, Erland},
  year = {2004},
  month = jan,
  journal = {Soil Biology and Biochemistry},
  volume = {36},
  number = {1},
  pages = {57--65},
  issn = {0038-0717},
  doi = {10.1016/j.soilbio.2003.08.019},
  urldate = {2018-08-10},
  abstract = {Eleven species of common fungi from compost were analysed for their content of ergosterol and phospholipid fatty acids (PLFAs) after growth on agar media. Mean content of ergosterol was 3.1 mg g-1dw of fungal mycelium (range 1--24 mg g-1dw). Total amount of PLFAs varied between 2.6 and 43.5 {$\mu$}molg-1dw of fungi (mean 14.9{$\mu$}molg-1dw). The most common PLFAs were 16:0, 18:2{$\omega$}6,9 and 18:1{$\omega$}9, comprising between 79 and 97mol\% of the total amount of PLFAs. The PLFA 18:2{$\omega$}6,9, suggested as a marker molecule for fungi, comprised between 36 and 61mol\% of the total PLFAs in the Ascomycetes, between 45 and 57mol\% in the Basidiomycetes and 12--22mol\% in the Zygomycetes. There was a good correlation between the content of the two fungal marker molecules, ergosterol and the PLFA 18:2{$\omega$}6,9, with a mean content of 1 mg ergosterol being equivalent to 2.1{$\mu$}mol of 18:2{$\omega$}6,9. Based on results from the fungal isolates, conversion factors were calculated (5.4 mg ergosterolg-1 biomass C and 11.8{$\mu$}mol 18:2{$\omega$}6,9 g-1 biomass C) and applied to compost samples in which both the ergosterol and the PLFA 18:2{$\omega$}6,9 content had been measured. This resulted in similar estimates of fungal biomass C using the two marker molecules, but was three to five times higher than total microbial biomass C calculated using ATP content in the compost. This could partly be explained by the fact that both of the markers used for fungal biomass are cell membrane constituents. Thus, the ergosterol and the PLFA content were related to the hyphal diameter of the fungi, where fungi with thinner hyphae had higher ergosterol concentrations than fungi with thicker hyphae. This could also partly explain the large interspecific variation in content of the two marker molecules.},
  keywords = {Compost,Conversion factor,Ergosterol,Phospholipid fatty acid},
  file = {/home/brfurnea/gdrive/Papers/Lab Methods/PLFA/Klamer_Bååth_2004_Estimation_of_conversion_factors_for_fungal_biomass_determination_in_compost(Soil_Biology_and_Biochemistry).pdf;/home/brfurnea/Zotero/storage/J9GSD3R9/S0038071703002931.html}
