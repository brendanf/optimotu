@article{kennedy2012,
  title = {Scaling up: Examining the Macroecology of Ectomycorrhizal Fungi},
  shorttitle = {Scaling Up},
  author = {Kennedy, Peter G. and Matheny, P. B. and Ryberg, K. M. and Henkel, Terry W. and Uehling, Jessie K. and Smith, Matthew E.},
  year = {2012},
