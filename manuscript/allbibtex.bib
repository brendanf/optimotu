  title = {Out of {{Asia}}: {{Biogeography}} of Fungal Populations Reveals {{Asian}} Origin of Diversification of the {{Laccaria}} Amethystina Complex, and Two New Species of Violet {{Laccaria}}},
  shorttitle = {Out of {{Asia}}},
  author = {Vincenot, Lucie and Popa, Flavius and Laso, Francisco and Donges, Kathrin and Rexer, Karl-Heinz and Kost, Gerhard and Yang, Zhu L. and Nara, Kazuhide and Selosse, Marc-Andr{\'e}},
