@article{hogberg2010,
  title = {Quantification of Effects of Season and Nitrogen Supply on Tree Below-Ground Carbon Transfer to Ectomycorrhizal Fungi and Other Soil Organisms in a Boreal Pine Forest},
  author = {H{\"o}gberg, Mona N. and Briones, Maria J. I. and Keel, Sonja G. and Metcalfe, Daniel B. and Campbell, Catherine and Midwood, Andrew J. and Thornton, Barry and Hurry, Vaughan and Linder, Sune and N{\"a}sholm, Torgny and H{\"o}gberg, Peter},
  year = {2010},
  journal = {New Phytologist},
  volume = {187},
  number = {2},
  eprint = {https://nph.onlinelibrary.wiley.com/doi/pdf/10.1111/j.1469-8137.2010.03274.x},
  pages = {485--493},
  doi = {10.1111/j.1469-8137.2010.03274.x},
  abstract = {Summary {$\bullet$} The flux of carbon from tree photosynthesis through roots to ectomycorrhizal (ECM) fungi and other soil organisms is assumed to vary with season and with edaphic factors such as nitrogen availability, but these effects have not been quantified directly in the field. {$\bullet$} To address this deficiency, we conducted high temporal-resolution tracing of 13C from canopy photosynthesis to different groups of soil organisms in a young boreal Pinus sylvestris forest. {$\bullet$} There was a 500\% higher below-ground allocation of plant C in the late (August) season compared with the early season (June). Labelled C was primarily found in fungal fatty acid biomarkers (and rarely in bacterial biomarkers), and in Collembola, but not in Acari and Enchytraeidae. The production of sporocarps of ECM fungi was totally dependent on allocation of recent photosynthate in the late season. There was no short-term (2 wk) effect of additions of N to the soil, but after 1 yr, there was a 60\% reduction of below-ground C allocation to soil biota. {$\bullet$} Thus, organisms in forest soils, and their roles in ecosystem functions, appear highly sensitive to plant physiological responses to two major aspects of global change: changes in seasonal weather patterns and N eutrophication.},
  keywords = {13CO2 labelling,boreal pine forest,ectomycorrhizal (ECM) fungi,nitrogen availability,seasonality,soil animals,soil CO2 efflux,soil microbial cytoplasm}
}
