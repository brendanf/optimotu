@article{baath2003,
  title = {Comparison of Soil Fungal/Bacterial Ratios in a {{pH}} Gradient Using Physiological and {{PLFA-based}} Techniques},
  author = {B{\aa}{\aa}th, E. and Anderson, T. -H.},
  year = {2003},
