  author = {{Garc{\'i}a-Montero}, Luis G. and {Valverde-Asenjo}, Inmaculada and Moreno, Domingo and D{\'i}az, Paloma and Hernando, Isabel and Menta, Cristina and Tarasconi, Katia},
