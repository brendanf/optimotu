@article{maba2015,
  title = {Phylogenetic and Microscopic Studies in the Genus {{{\emph{Lactifluus}}}} ({{{\emph{Basidiomycota}}}}{\emph{, }}{{{\emph{Russulales}}}}) in {{West Africa}}, Including the Description of Four New Species},
