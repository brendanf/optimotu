@article{adl2012,
  title = {The {{Revised Classification}} of {{Eukaryotes}}},
  author = {Adl, Sina M. and Simpson, Alastair G. B. and Lane, Christopher E. and Luke{\v s}, Julius and Bass, David and Bowser, Samuel S. and Brown, Matthew W. and Burki, Fabien and Dunthorn, Micah and Hampl, Vladimir and Heiss, Aaron and Hoppenrath, Mona and Lara, Enrique and {\noopsort{gall}}le Gall, Line and Lynn, Denis H. and McManus, Hilary and Mitchell, Edward A. D. and Mozley-Stanridge, Sharon E. and Parfrey, Laura W. and Pawlowski, Jan and Rueckert, Sonja and Shadwick, Laura and Schoch, Conrad L. and Smirnov, Alexey and Spiegel, Frederick W.},
