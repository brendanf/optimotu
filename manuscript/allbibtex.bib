  title = {Phylogenetic Analysis of 73 060 Taxa Corroborates Major Eukaryotic Groups},
  author = {Goloboff, Pablo A. and Catalano, Santiago A. and Marcos Mirande, J. and Szumik, Claudia A. and Salvador Arias, J. and K{\"a}llersj{\"o}, Mari and Farris, James S.},
