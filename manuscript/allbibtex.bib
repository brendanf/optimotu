@article{bahram2022,
  title = {Structure and Function of the Soil Microbiome Underlying {{N2O}} Emissions from Global Wetlands},
  author = {Bahram, Mohammad and Espenberg, Mikk and P{\"a}rn, Jaan and {Lehtovirta-Morley}, Laura and Anslan, Sten and Kasak, Kuno and K{\~o}ljalg, Urmas and Liira, Jaan and Maddison, Martin and Moora, Mari and Niinemets, {\"U}lo and {\"O}pik, Maarja and P{\"a}rtel, Meelis and Soosaar, Kaido and Zobel, Martin and Hildebrand, Falk and Tedersoo, Leho and Mander, {\"U}lo},
  year = {2022},
