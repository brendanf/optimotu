@article{guissou2008,
  title = {Declining {{Wild Mushroom Recognition}} and {{Usage}} in {{Burkina Faso}}},
  author = {Guissou, K. Marie Laure and Lykke, Anne Mette and Sankara, Philippe and Guinko, Sita},
  year = {2008},
