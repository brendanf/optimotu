  file = {/home/brfurnea/gdrive/Papers/Computer Methods/Bioinformatics/Databases/Philippoussis_etal_2001_Bioconversion_of_agricultural_lignocellulosic_wastes_through_the_cultivation_of(World_Journal_of_Microbiology_and_Biotechnology).pdf}
