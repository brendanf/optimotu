@article{rucci2019,
  title = {{{SWIMM}} 2.0: {{Enhanced Smith}}--{{Waterman}} on {{Intel}}'s {{Multicore}} and {{Manycore Architectures Based}} on {{AVX-512 Vector Extensions}}},
  shorttitle = {{{SWIMM}} 2.0},
  author = {Rucci, Enzo and Garcia Sanchez, Carlos and Botella Juan, Guillermo and Giusti, Armando De and Naiouf, Marcelo and {Prieto-Matias}, Manuel},
