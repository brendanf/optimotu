@article{pineiro2024,
  title = {Efficient Phylogenetic Tree Inference for Massive Taxonomic Datasets: Harnessing the Power of a Server to Analyze 1 Million Taxa},
  shorttitle = {Efficient Phylogenetic Tree Inference for Massive Taxonomic Datasets},
  author = {Pi{\~n}eiro, C{\'e}sar and Pichel, Juan C},
