@article{shokralla2012,
  title = {Next-Generation Sequencing Technologies for Environmental {{DNA}} Research},
  author = {Shokralla, Shadi and Spall, Jennifer L. and Gibson, Joel F. and Hajibabaei, Mehrdad},
