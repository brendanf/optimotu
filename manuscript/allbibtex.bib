@article{jacobson1993,
  title = {Randomly Amplified Polymorphic {{DNA}} Markers Are Superior to Somatic Incompatibility Tests for Discriminating Genotypes in Natural Populations of the Ectomycorrhizal Fungus {{Suillus}} Granulatus},
  author = {Jacobson, K. M. and Miller, O. K. and Turner, B. J.},
