@article{henras2015,
  title = {An Overview of Pre-Ribosomal {{RNA}} Processing in Eukaryotes},
  author = {Henras, Anthony K. and Plisson-Chastang, C{\'e}lia and O'Donohue, Marie-Fran{\c c}oise and Chakraborty, Anirban and Gleizes, Pierre-Emmanuel},
  year = {2015},
  journal = {Wiley Interdisciplinary Reviews: RNA},
