@article{avis2003,
  title = {Long-Term Increase in Nitrogen Supply Alters above- and below-Ground Ectomycorrhizal Communities and Increases the Dominance of {{{\emph{Russula}}}} Spp. in a Temperate Oak Savanna},
  author = {Avis, Peter G. and McLaughlin, David J. and Dentinger, Bryn C. and Reich, Peter B.},
  year = {2003},
  month = oct,
  journal = {New Phytologist},
  volume = {160},
  number = {1},
  pages = {239--253},
  issn = {1469-8137},
  doi = {10.1046/j.1469-8137.2003.00865.x},
  urldate = {2018-08-17},
  abstract = {{$\bullet$} Here we examine the effects of increased nitrogen (N) supply on the ectomycorrhizal fungal communities of a temperate oak savanna. {$\bullet$} In a 16-yr N-addition experiment in which replicate 1000 m2 plots received 0, 5.4 or 17 g N m-2 yr-1, ectomycorrhizal sporocarp production was measured in the 14th, 15th and 16th year of fertilization. Ectomycorrhizal fungi (EMF) colonizing roots were examined by morphotyping-PCR-RFLP and sequence analysis in the 14th and 15th year of fertilization. {$\bullet$} Total sporocarp richness was reduced by {$>$} 50\% in both fertilization treatments in all 3 yrs, whereas Russula spp. produced approx. five times more sporocarps with 17 g N m-2 yr-1. Below-ground, treatment-scale species richness and species area curves were lower with 17 g N m-2 yr-1 but richness, diversity indices and evenness at smaller spatial scales were not. Dominant fungi colonizing roots included Cenococcum geophilum, common in all treatments, Cortinarius spp., dominant in unfertilized plots, and Russula spp., dominant with 17 g N m-2 yr-1. {$\bullet$} Communities of EMF in this temperate deciduous ecosystem responded to N addition similarly to those of coniferous ecosystems in that increased N supply altered EMF diversity and community composition but differently in that dominance of Russula spp. increased.},
  langid = {english},
  keywords = {Cenococcum,Cortinarius,ectomycorrhizal fungi (EMF),fungal communities,nitrogen fertilization,oak savanna,Russula},
  file = {/home/brfurnea/gdrive/Papers/Avis_etal_2003_Long-term_increase_in_nitrogen_supply_alters_above-_and_below-ground(New_Phytologist).pdf;/home/brfurnea/Zotero/storage/N75W64P5/j.1469-8137.2003.00865.html}
}
