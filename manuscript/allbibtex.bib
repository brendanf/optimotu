  author = {Speranskaya, Anna S. and Khafizov, Kamil and Ayginin, Andrey A. and Krinitsina, Anastasia A. and Omelchenko, Denis O. and Nilova, Maya V. and Severova, Elena E. and Samokhina, Evgeniya N. and Shipulin, German A. and Logacheva, Maria D.},
