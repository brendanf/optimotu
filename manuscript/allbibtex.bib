@article{bouckaert2019,
  title = {{{BEAST}} 2.5: {{An}} Advanced Software Platform for {{Bayesian}} Evolutionary Analysis},
  shorttitle = {{{BEAST}} 2.5},
  author = {Bouckaert, Remco and Vaughan, Timothy G. and {Barido-Sottani}, Jo{\"e}lle and Duch{\^e}ne, Sebasti{\'a}n and Fourment, Mathieu and Gavryushkina, Alexandra and Heled, Joseph and Jones, Graham and K{\"u}hnert, Denise and Maio, Nicola De and Matschiner, Michael and Mendes, F{\'a}bio K. and M{\"u}ller, Nicola F. and Ogilvie, Huw A. and {\noopsort{plessis}}du Plessis, Louis and Popinga, Alex and Rambaut, Andrew and Rasmussen, David and Siveroni, Igor and Suchard, Marc A. and Wu, Chieh-Hsi and Xie, Dong and Zhang, Chi and Stadler, Tanja and Drummond, Alexei J.},
