@article{newell1987,
  title = {Ergosterol {{Content}} of {{Salt-Marsh Fungi}}: {{Effect}} of {{Growth Conditions}} and {{Mycelial Age}}},
  shorttitle = {Ergosterol {{Content}} of {{Salt-Marsh Fungi}}},
  author = {Newell, Steven Y. and Miller, J. David and Fallon, Robert D.},
  year = {1987},
