@article{bohmann2022,
  title = {Strategies for Sample Labelling and Library Preparation in {{DNA}} Metabarcoding Studies},
  author = {Bohmann, Kristine and Elbrecht, Vasco and Car{\o}e, Christian and Bista, Iliana and Leese, Florian and Bunce, Michael and Yu, Douglas W. and Seymour, Mathew and Dumbrell, Alex J. and Creer, Simon},
  year = {2022},
