@article{meterSalinizationAltersEcosystem2011,
  title = {Salinization Alters Ecosystem Structure in Urban Stormwater Detention Ponds},
  author = {Meter, Robin J. Van and Swan, Christopher M. and Snodgrass, Joel W.},
  year = {2011},
  month = nov,
  journal = {Urban Ecosystems},
  volume = {14},
