  booktitle = {Edible {{Ectomycorrhizal Mushrooms}}: {{Current Knowledge}} and {{Future Prospects}}},
  author = {Morte, Asunci{\'o}n and Andrino, Alberto and Honrubia, Mario and {Navarro-R{\'o}denas}, Alfonso},
