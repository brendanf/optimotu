  author = {Dong, Wei and Wang, Bin and Hyde, Kevin D. and McKenzie, Eric H. C. and Raja, Huzefa A. and Tanaka, Kazuaki and {Abdel-Wahab}, Mohamed A. and {Abdel-Aziz}, Faten A. and Doilom, Mingkwan and Phookamsak, Rungtiwa and Hongsanan, Sinang and Wanasinghe, Dhanushka N. and Yu, Xian-Dong and Wang, Gen-Nuo and Yang, Hao and Yang, Jing and Thambugala, Kasun M. and Tian, Qing and Luo, Zong-Long and Yang, Jian-Bo and Miller, Andrew N. and Fournier, Jacques and Boonmee, Saranyaphat and Hu, Dian-Ming and Nalumpang, Sarunya and Zhang, Huang},
