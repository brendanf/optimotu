@article{robert2013,
  title = {{{MycoBank}} Gearing up for New Horizons},
  author = {Robert, Vincent and Vu, Duong and Amor, Ammar Ben Hadj and {\noopsort{wiele}}van de Wiele, Nathalie and Brouwer, Carlo and Jabas, Bernard and Szoke, Szaniszlo and Dridi, Ahmed and Triki, Maher and {\noopsort{daoud}}ben Daoud, Samy and Chouchen, Oussema and Vaas, Lea and {\noopsort{cock}}de Cock, Arthur and Stalpers, Joost A. and Stalpers, Dora and Verkley, Gerard J. M. and Groenewald, Marizeth and {\noopsort{santos}}dos Santos, Felipe Borges and Stegehuis, Gerrit and Li, Wei and Wu, Linhuan and Zhang, Run and Ma, Juncai and Zhou, Miaomiao and Gorj{\'o}n, Sergio P{\'e}rez and Eurwilaichitr, Lily and Ingsriswang, Supawadee and Hansen, Karen and Schoch, Conrad and Robbertse, Barbara and Irinyi, Laszlo and Meyer, Wieland and Cardinali, Gianluigi and Hawksworth, David L. and Taylor, John W. and Crous, Pedro W.},
